<Window x:Class="Stresstester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stresstester"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="CPU Stresstest" HorizontalAlignment="Center" Margin="0,191,0,0" VerticalAlignment="Top" Width="106" Click="CPUStresstestButton" Height="42"/>
        <TextBox x:Name="NumberOfThreads" HorizontalAlignment="Center" Height="42" Margin="0,255,0,0" TextWrapping="Wrap" Text="Number of threads" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="CurrentThreadsLabel" Content="Current threads" HorizontalAlignment="Center" Margin="0,136,0,0" VerticalAlignment="Top" Width="142" Height="36"/>

    </Grid>
</Window>
